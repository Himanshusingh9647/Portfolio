{
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)\\.js", "dest": "/$1.js" },
    { "src": "/(.*)\\.css", "dest": "/$1.css" },
    { "src": "/(.*)\\.svg", "dest": "/$1.svg" },
    { "src": "/(.*)\\.png", "dest": "/$1.png" },
    { "src": "/(.*)\\.jpg", "dest": "/$1.jpg" },
    { "src": "/(.*)\\.mp4", "dest": "/$1.mp4" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
